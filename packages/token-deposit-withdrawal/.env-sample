# This is a sample .env file for use in local development.
# Duplicate this file as .env here

# Your Private key
DEVNET_PRIVKEY="0x..."

# Hosted Aggregator Node (JSON-RPC Endpoint)
L2RPC=https://rinkeby.arbitrum.io/rpc

# Ethereum RPC; i.e., for rinkeby https://rinkeby.infura.io/v3/<your infura key>
L1RPC=https://rinkeby.infura.io/v3/...

DAPP_CONTRACT=0x69Ab4F50E5cBD3827E2272416422e666B9c0d24B

# If ETH_FLAG = true, then transfer ETH instead of tokens
ETH_FLAG=
DEPOSIT_AMOUNT=500
ETH_DEPOSIT=0.0001
WITHDRAW_AMOUNT=500
ETH_WITHDRAWAL=0.000001

FACTORY_CONTRACT=
MASTER_CONTRACT=
CHILD_CONTRACT=